manifest {
    mainScript = 'chipseq.nf'
}

includeConfig "$baseDir/params.config"
 

process {
    queue='short-sl7'
    memory='8G'
    cpus='1'
    time='6h'
    scratch = false

    withLabel: big_mem_cpus {
        cpus = 8
        memory = '64G'
        queue = 'short-sl7,long-sl7,mem_1tb,mem_512,mem_256'
    }
 
     withLabel: big_mem {
        cpus = 1
        memory = '40G'
        queue = 'short-sl7,long-sl7,mem_1tb,mem_512,mem_256'
    }
 
}

process.container = '/software/bi/biocore_tools/git/singularity_conversion/singularity/biocorecrg_vectorqc-2018-08-16-9835276b3c58.img'
singularity.enabled = true



